#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hermit/Documents/jsProject/mywork/myworks/node_modules/.pnpm/typeorm@0.3.14_mysql2@2.3.3_redis@4.6.5_ts-node@10.9.1/node_modules/typeorm/node_modules:/Users/hermit/Documents/jsProject/mywork/myworks/node_modules/.pnpm/typeorm@0.3.14_mysql2@2.3.3_redis@4.6.5_ts-node@10.9.1/node_modules:/Users/hermit/Documents/jsProject/mywork/myworks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hermit/Documents/jsProject/mywork/myworks/node_modules/.pnpm/typeorm@0.3.14_mysql2@2.3.3_redis@4.6.5_ts-node@10.9.1/node_modules/typeorm/node_modules:/Users/hermit/Documents/jsProject/mywork/myworks/node_modules/.pnpm/typeorm@0.3.14_mysql2@2.3.3_redis@4.6.5_ts-node@10.9.1/node_modules:/Users/hermit/Documents/jsProject/mywork/myworks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
